<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calldriver"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14" 
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Notitle.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Notitle.Fullscreen" >
        </activity>
        <activity
            android:name=".DriverActivity"
            android:theme="@style/Theme.Fullscreen" >
        </activity>
        <activity
            android:name=".Driver_setingActivity"
            android:label="@string/title_activity_dirver_seting"
            android:theme="@style/Theme.Notitle.Fullscreen" >
        </activity>
        <activity
            android:name=".ClientActivity"
            android:label="@string/title_activity_client" android:theme="@style/Theme.Fullscreen">
        </activity>
        <activity
            android:name=".DriverShowActivity"
            android:label="@string/title_activity_driver_show">
        </activity>
    </application>

</manifest>
